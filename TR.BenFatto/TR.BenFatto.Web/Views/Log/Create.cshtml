@model TR.BenFatto.Application.ViewModels.UserLogViewModel

@{ ViewData["Title"] = "Cadastrar Log"; }

<h2>@ViewData["Title"]</h2>

<form asp-action="Create">
    <div class="form-horizontal">
        <hr />
        <vc:summary />
        <div class="form-group">
            <label asp-for="IpAdress" ></label>
            <div class="col-md-10">
                <input asp-for="IpAdress" class="form-control" />
                <span asp-validation-for="IpAdress" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="NormalizedDate" ></label>
            <div class="col-md-10">
                <input asp-for="NormalizedDate" class="form-control" />
                <span asp-validation-for="NormalizedDate" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="UserAgent" ></label>
            <div class="col-md-10">
                <input asp-for="UserAgent" class="form-control" />
                <span asp-validation-for="UserAgent" class="text-danger"></span>
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Salvar" class="btn btn-success" />
                <a asp-action="Index" class="btn btn-info">Voltar</a>
            </div>
        </div>
    </div>
</form>