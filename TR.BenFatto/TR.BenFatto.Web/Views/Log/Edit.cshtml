@model TR.BenFatto.Application.ViewModels.UserLogViewModel

@{ ViewData["Title"] = "Alterar log"; }

@if (ViewBag.Erro != null)
{
<div class="alert alert-danger">
    <p><strong>Erro:</strong> @ViewBag.Erro.ToString()</p>
</div>}

<h2>@ViewData["Title"]</h2>

<form asp-action="Edit">
    <div class="form-horizontal">
        <hr />
        <vc:summary />
        <div class="form-group">
            <label asp-for="IpAdress" class="col-md-2 custom-control-label"></label>
            <div class="col-md-10">
                <input asp-for="IpAdress" class="form-control" />
                <span asp-validation-for="IpAdress" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="NormalizedDate" class="col-md-2 custom-control-label"></label>
            <div class="col-md-10">
                <input asp-for="NormalizedDate" class="form-control" />
                <span asp-validation-for="NormalizedDate" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="UserAgent" class="col-md-2 custom-control-label"></label>
            <div class="col-md-10">
                <input asp-for="UserAgent" class="form-control" />
                <span asp-validation-for="UserAgent" class="text-danger"></span>
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Alterar" class="btn btn-success" />
                <a asp-action="Index" class="btn btn-info">Voltar</a>
            </div>
        </div>
    </div>
</form>